{
  "name": "design-profile-generator",
  "description": "Quickly generate a graphic design profile (as CSS)",
  "version": "2.2.2",
  "license": "ISC",
  "homepage": "http://tomsoderlund.github.io/design-profile-generator",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "ignore": [
      ".next"
    ],
    "globals": [
      "beforeAll",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jasmine",
      "spyOn"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "3.1.2",
    "simple-browser-session": "^1.0.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "standard": "^14.3.1"
  }
}
